#, markdown-text, no-wrap
#
msgid ""
"ps | awk '/[a]uto_run.sh/ {print $1}' | while read PID; do kill -9 $PID; "
"done;\n"
"ps | awk '/[j]co_server/ {print $1}' | xargs kill -9 &>/dev/null\n"
"echo 'V'>/dev/watchdog\n"
"echo 'V'>/dev/watchdog0\n"
"rm '/opt/media/mmcblk0p1/cid.txt' &>/dev/null\n"
msgstr "ps | awk '/[a]uto_run.sh/ {print $1}' | while read PID; do kill -9 $PID; done;\nps | awk '/[j]co_servilo / {print}' | xargs kill -9 \u0026>/ dev / null\necho 'V'>/ dev / watchdog\necho 'V'>/ dev / watchdog0\nrm '/opt/media/mmcblk0p1/cid.txt' \u0026>/ dev / null\n"

