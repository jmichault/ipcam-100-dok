#. type: Title #
#: kruc-kompili.md:16
#, fuzzy, no-wrap
msgid "installation des outils de compilation :"
msgstr "instalado de kompililoj:"

#. type: Title ##
#: kruc-kompili.md:17
#, fuzzy, no-wrap
msgid "option 1 : avec docker (plus rapide à installer, plus lent à exécuter)"
msgstr "opcio 1: kun docker (pli rapida por instali, pli malrapida por funkciigi)"

#. type: Fenced code block
#: kruc-kompili.md:19
#, no-wrap
msgid ""
"sudo apt install docker.io\n"
"sudo usermod -aG docker mia_uzanto\n"
" se déconnecter puis se reconnecter\n"
"mkdir ~/dafang\n"
"cd ~/dafang\n"
"docker run --rm -ti -v $(pwd):/root/ daviey/dafang-cross-compile:latest\n"
msgstr ""
"sudo apt install docker.io\n"
"sudo usermod -aG docker mia_uzanto\n"
" se déconnecter puis se reconnecter\n"
"mkdir ~/dafang\n"
"cd ~/dafang\n"
"docker run --rm -ti -v $(pwd):/root/ daviey/dafang-cross-compile:latest\n"

#. type: Title ##
#: kruc-kompili.md:28
#, fuzzy, no-wrap
msgid "option 2 : en local (plus long à installer, plus rapide à exécuter)"
msgstr "opcio 2: loke (pli longa por instali, pli rapida por ekzekuti)"

#. type: Fenced code block
#: kruc-kompili.md:30
#, no-wrap
msgid ""
"sudo apt install build-essential git gcc-mips-linux-gnu autoconf libtool cmake ftp-upload u-boot-tools\n"
"mkdir -p ~/dafang/Main/mips-gcc472-glibc216-64bit\n"
"ln -s /usr/bin ~/dafang/Main/mips-gcc472-glibc216-64bit\n"
msgstr ""
"sudo apt install build-essential git gcc-mips-linux-gnu autoconf libtool cmake ftp-upload u-boot-tools\n"
"mkdir -p ~/dafang/Main/mips-gcc472-glibc216-64bit\n"
"ln -s /usr/bin ~/dafang/Main/mips-gcc472-glibc216-64bit\n"

#. type: Title #
#: kruc-kompili.md:36
#, fuzzy, no-wrap
msgid "compilation des librairies et de _v4l2rtspserver-master_"
msgstr "kompilo de bibliotekoj kaj _v4l2rtspserver-master_"

#. type: Fenced code block
#: kruc-kompili.md:38
#, no-wrap
msgid ""
"git clone --recurse-submodules https://github.com/Dafang-Hacks/Main.git\n"
"cd Main\n"
"./compile_libraries.sh\n"
"cd v4l2rtspserver-master\n"
"./compile.sh\n"
msgstr ""
"git clone --recurse-submodules https://github.com/Dafang-Hacks/Main.git\n"
"cd Main\n"
"./compile_libraries.sh\n"
"cd v4l2rtspserver-master\n"
"./compile.sh\n"

